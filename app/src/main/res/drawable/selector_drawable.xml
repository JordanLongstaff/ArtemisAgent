<?xml version="1.0" encoding="utf-8"?>
<inset xmlns:android="http://schemas.android.com/apk/res/android"
    android:insetLeft="@dimen/buttonInsetHorizontal"
    android:insetTop="@dimen/buttonInsetVertical"
    android:insetRight="@dimen/buttonInsetHorizontal"
    android:insetBottom="@dimen/buttonInsetVertical">
    <shape android:shape="rectangle">
        <stroke android:width="@dimen/paddingSmall" android:color="@color/button_stroke" />
        <solid android:color="@color/selector_background" />
        <padding android:left="@dimen/buttonPaddingHorizontal"
            android:top="@dimen/buttonPaddingVertical"
            android:right="@dimen/buttonPaddingHorizontal"
            android:bottom="@dimen/buttonPaddingVertical" />
    </shape>
</inset>
